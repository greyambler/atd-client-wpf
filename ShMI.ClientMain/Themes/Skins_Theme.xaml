    <LinearGradientBrush x:Key="stroke_Vint" StartPoint="0.1,0" EndPoint="0.5,1" >
        <LinearGradientBrush.RelativeTransform>
            <TransformGroup>
                <ScaleTransform CenterY="0.5" CenterX="0.5" />
                <SkewTransform CenterY="0.5" CenterX="0.5" />
                <RotateTransform Angle="-18.435" CenterY="0.5" CenterX="0.5" />
                <TranslateTransform />
            </TransformGroup>
        </LinearGradientBrush.RelativeTransform>
        <GradientStop Color="{Binding Color, Source={StaticResource vint_black}}" Offset="1" />
        <GradientStop Color="{Binding Color, Source={StaticResource vint_main}}"/>
        <GradientStop Color="{Binding Color, Source={StaticResource vint_main_g1}}" Offset="0.407" />
        <GradientStop Color="{Binding Color, Source={StaticResource vint_black_g1}}" Offset="0.911" />
    </LinearGradientBrush>
